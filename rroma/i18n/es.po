#. module: rroma
#: model:mail.template,body_html:rroma.invoiced_ticket_request_email_template
msgid ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"    % set repair = object.fsm_task_ids.repair_id\n"
"    Dear ${object.sudo().partner_id.name or 'Madam/Sir'},<br/><br/>\n"
"    This automatic message informs you that we have invoiced your ticket ${object.name}.<br/>\n"
"    % if repair.name:\n"
"    The repair order: ${repair.name} has been closed and invoiced correctly.<br/><br/>\n"
"    % endif\n"
"    Kind regards,<br/><br/>\n"
"    ${object.team_id.name or 'Helpdesk'} Team.\n"
"</div>\n"
"        "
msgstr ""
"<?xml version=\"1.0\"?>\n"
"<div>\n"
"    % set repair = object.fsm_task_ids.repair_id\n"
"    Estimado ${object.sudo().partner_id.name or 'Señora/Señor'},<br/><br/>\n"
"    Este mensaje automático le informa que hemos facturado su ticket ${object.name}.<br/>\n"
"    % if repair.name:\n"
"    La orden de reparación: ${repair.name} ha sido cerrado y facturado correctamente.<br/><br/>\n"
"    % endif\n"
"    Saludos cordiales,<br/><br/>\n"
"    ${object.team_id.name or 'Mesa de ayuda'} Equipo.\n"
"</div>\n"
"        "

