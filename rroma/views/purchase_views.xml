<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="purchase_form_view_inherit_rroma" model="ir.ui.view">
        <field name="name">purchase.order.form.inherit.rroma</field>
        <field name="model">purchase.order</field>
        <field name="inherit_id" ref="purchase.purchase_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='origin']" position="after">
                <field name="shipping_instructions"/>
                <field name="door_to_door"/>
                <field name="door_to_port"/>
                <field name="alternate_shipping_address_id"/>
            </xpath>
        </field>
    </record>
</odoo>
